#include <stdio.h>
#include <stdlib.h>
#include <time.h>
int main (){
int a,b ;
printf ("veuillez entrer un nombre ")
scanf ("%d",&a);
printf ("veuillez entrer un nombre ")
scanf ("%d",&b);
if (a-b<0)
printf ("%d < %d",a,b);
else if (a-b>0)
printf ("%d == %d",a,b);
else 
printf ("%d = %d",a,b);

return 0;
}


